<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE tiles-definitions PUBLIC  
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd"> 
       
<tiles-definitions>
	  <!-- Definition -->
	  <definition name="template" template="/WEB-INF/jsp/template.jsp">
		    <put-attribute name="header" value="/WEB-INF/jsp/tiles/header.jsp" />
		    <put-attribute name="menu" value="/WEB-INF/jsp/tiles/menu.jsp" />
		    <put-attribute name="body" value="" />
		    <put-attribute name="footer" value="/WEB-INF/jsp/tiles/footer.jsp" />
	  </definition>
	    
	  <!-- Login -->
	  <definition name="login" template="/WEB-INF/jsp/login.jsp">
		    <put-attribute name="footer" value="/WEB-INF/jsp/tiles/footer.jsp" />
	  </definition>
	  
	  <!-- Home -->
	  <definition name="home" extends="template">
		    <put-attribute name="body" value="/WEB-INF/jsp/home.jsp" />
	  </definition>
	  
	  <!-- Keyword-ranking -->
	  <definition name="keyword-ranking" extends="template">
		    <put-attribute name="body" value="/WEB-INF/jsp/keyword-ranking/keyword-ranking.jsp" />
	  </definition>
	  
	  <!-- keyword-ranking/websites -->
	  <definition name="keyword-ranking/websites" extends="template">
		    <put-attribute name="body" value="/WEB-INF/jsp/keyword-ranking/websites.jsp" />
	  </definition>
	  
	  <!-- keyword-ranking/website -->
	  <definition name="keyword-ranking/website" extends="template">
		    <put-attribute name="body" value="/WEB-INF/jsp/keyword-ranking/website.jsp" />
	  </definition>
	  
	  <!-- keyword-ranking/options -->
	  <definition name="keyword-ranking/options" extends="template">
		    <put-attribute name="body" value="/WEB-INF/jsp/keyword-ranking/options.jsp" />
	  </definition>
	  
</tiles-definitions>